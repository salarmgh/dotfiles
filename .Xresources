Xcursor.size: 16
XTerm*renderFont: true
XTerm*loginshell: true
XTerm*faceName: Hack
XTerm*faceSize: 14
XTerm*selectToClipboard: true
XTerm*vt100*metaSendsEscape: true
XTerm*termName: xterm-256color
XTerm.vt100.locale: true
xTerm.vt100.blink: true
XTerm.vt100.saveLines: 10000000
XTerm*VT100*translations: #override \n\
    Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
        Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2
